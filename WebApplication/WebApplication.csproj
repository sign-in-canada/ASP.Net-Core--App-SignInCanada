        <DependentUpon>Program.resx</DependentUpon>
      </Compile>
      <Compile Update="Resources\Pages\IndexModel.Designer.cs">
        <AutoGen>True</AutoGen>
        <DependentUpon>IndexModel.resx</DependentUpon>
        <DesignTime>True</DesignTime>
      </Compile>
